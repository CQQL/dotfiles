Config { alignSep = "}{",
         bgColor = "#1e1e1e",
         fgColor = "#a7a7a7",
         commands = [ Run BatteryP ["BAT0", "BAT1", "BAT2"]
                                   ["-t", "<acstatus> <left>% <timeleft>",
                                    "--",
                                    "-O", "+",
                                    "-i", "=",
                                    "-o", "-"]
                                   10,
                      Run MultiCpu ["-t", "CPU: <total>%"] 10,
                      Run Date "%d.%m.%Y %H:%M" "datetime" 600,
                      Run Network "enp2s0" ["-t", "<dev>"] 10,
                      Run Wireless "wlp1s0" ["-t", "<essid>"] 10,
                      Run Kbd [],
                      Run StdinReader
                    ],
         template = "%StdinReader% } %datetime% { %kbd% <fc=#5f5a60>/</fc> %multicpu% <fc=#5f5a60>/</fc> %battery% <fc=#5f5a60>/</fc> %enp2s0% %wlp1s0wi%"
       }
