URxvt*jumpScroll: False
URxvt*font: xft:Fantasque Sans Mono:size=13:hinting=false
URxvt*scrollBar: False
URxvt*scollTtyOutput: False
URxvt*scollWithBuffer: True
URxvt*scollTtyKeypress: True
URxvt*internalBorder: 10px
! Remove a black border around the window in WMs that respect size hints
URxvt*externalBorder: 10px

Emacs.menuBar: off
Emacs.toolBar: off
Emacs.verticalScrollBars: off
Emacs.horizontalScrollBars: off
Emacs.font: Fantasque Sans Mono-13:autohint=yes

!!! Terminal colors
!! gruvbox theme
!! https://github.com/morhetz/gruvbox-contrib/blob/master/xresources/gruvbox-dark.xresources

! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
